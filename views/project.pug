extends layout

block content
	#pistilligallery.container
		//Heading
		.row
			.col-lg-12.text-center
				h5.jewelrygallery MY PROJECTS
				h4.jewelrygallery I Will Update More Collection!
		// The project information
		.row
			.col-lg-3.col-md-6.col-sm-12.pistilligallery-item
				img.img-fluid(src=`../${project.image}`, alt=`${project.title}`)
				.pistilligallery-caption
					h3.text-muted= project.title

